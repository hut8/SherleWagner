<div class="col-md-4">
  <p><%= showroom.header %></p>
  <h2><%= showroom.name %></h2>
  <%= simple_format(showroom.address) %>
  <p><%= showroom.phone %></p>
  <p><%= showroom.fax %></p>
  <p><%= showroom.website %></p> 
  <ul>
    <% showroom.emails.each do |email| %>
      <li>email</li>
    <% end %>
  </ul>
</div>
