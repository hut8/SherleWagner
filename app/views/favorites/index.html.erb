<div class="prod_wrapper">
  <h1>favorites</h1>
  <ul>
  <% @favorites.each do |favorite| %>
    <li>
      <%= link_to favorite.product_name,
       favorite.url,
        id:favorite.css_id,
        class: 'favorite' %>
      <%= link_to "delete", favorites_path, method: 'delete', id: favorite.id %>
    </li>
  <% end %>
  </ul>
</div>
